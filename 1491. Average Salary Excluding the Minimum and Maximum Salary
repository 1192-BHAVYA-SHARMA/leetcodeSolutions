class Solution {
    public double average(int[] salary) {
        int n = salary.length;
        Arrays.sort(salary);
        int  sum=0;
        for(int i=1;i<n-1;i++){
            sum = sum +salary[i];
        }
        double sum1 = sum;
        double sum2=sum1/(n-2);
        return sum2;
    }
}
